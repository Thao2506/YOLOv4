    "            print(f\"[{epoch + 1}, {i + 1:5d}] loss: {running_loss / 100:.3f}\")\n",
    "            running_loss = 0.0\n",
    "\n",
    "end_time = time.time()\n",
    "print(f\"\\n✅ Training completed in {end_time - start_time:.2f} seconds\")\n",
